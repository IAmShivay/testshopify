  "bugs": {
    "url": "https://github.com/IAmShivay/testshopify/issues"
  },
  "homepage": "https://github.com/IAmShivay/testshopify#readme",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/cli": "^4.1.11",
    "alpinejs": "^3.14.9",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "esbuild": "^0.25.5"
  }
}
